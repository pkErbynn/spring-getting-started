<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

<bean id="humanBean" class="io.turntabl.springgettingstarted.HumanBean">
    <property name="FName" value="john" />
    <property name="LName" value="erbynn" />
</bean>

<bean id="bestie" class="io.turntabl.springgettingstarted.BestieBean">
    <property name="firstName" value="tetteh" />
    <property name="LastName" value="nortey" />
    <property name="Relationship" value="intimate" />
</bean>

</beans>

